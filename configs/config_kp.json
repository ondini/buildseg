{
    "name": "SolAR_KPdet",
    "device": "cuda:1",
    "gpus": [],
    "model": {
        "type": "MaxVitUnet",
        "args": {}
    },
    "train_data_loader": {
        "type": "SEA_AIDataLoader",
        "args": {
            "coco_root": "/home/kafkaon1/Dev/data/COCO_KPTS_2103",
            "ann_file": "annotations_train.json",
            "img_prefix": "train_imgs",
            "batch_size": 4,
            "shuffle": true,
            "num_workers": 2,
            "augmentation": false,
            "size_coefficient": 1
        }
    },
    "val_data_loader": {
        "type": "SEA_AIDataLoader",
        "args": {
            "coco_root": "/home/kafkaon1/Dev/data/COCO_KPTS_2103",
            "ann_file": "annotations_test.json",
            "img_prefix": "test_imgs",
            "batch_size": 4,
            "shuffle": true,
